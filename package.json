{
  "name": "@neuropulse/core",
  "version": "2.1.0",
  "description": "Advanced AI-enhanced wallet tracking and signal processing system for Solana blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc && webpack --mode production",
    "build:dev": "tsc && webpack --mode development",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.test.ts'",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "docker:build": "docker build -t neuropulse:latest .",
    "docker:run": "docker-compose up -d",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "generate": "prisma generate",
    "prepare": "husky install",
    "release": "changeset publish",
    "version": "changeset version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neuropulse/core.git"
  },
  "keywords": [
    "solana",
    "blockchain",
    "wallet-tracking",
    "ai",
    "machine-learning",
    "signal-processing",
    "cryptocurrency",
    "defi",
    "neural-network",
    "real-time",
    "websocket",
    "typescript"
  ],
  "author": "NeuroPulse Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neuropulse/core/issues"
  },
  "homepage": "https://github.com/neuropulse/core#readme",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "@metaplex-foundation/js": "^0.19.5",
    "@coral-xyz/anchor": "^0.29.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@tensorflow/tfjs-node-gpu": "^4.15.0",
    "onnxruntime-node": "^1.16.3",
    "brain.js": "^2.0.0-beta.23",
    "ml-matrix": "^6.11.0",
    "simple-statistics": "^7.8.3",
    "express": "^4.18.2",
    "fastify": "^4.25.2",
    "ws": "^8.16.0",
    "socket.io": "^4.6.1",
    "bull": "^4.12.0",
    "bullmq": "^5.1.1",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "mongoose": "^8.0.3",
    "pg": "^8.11.3",
    "kafkajs": "^2.2.4",
    "amqplib": "^0.10.3",
    "axios": "^1.6.3",
    "node-fetch": "^3.3.2",
    "got": "^14.0.0",
    "ethers": "^6.9.2",
    "web3": "^4.3.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "yup": "^1.3.3",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "pino": "^8.17.2",
    "morgan": "^1.10.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "argon2": "^0.31.2",
    "uuid": "^9.0.1",
    "nanoid": "^5.0.4",
    "dayjs": "^1.11.10",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "ramda": "^0.29.1",
    "rxjs": "^7.8.1",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "node-cron": "^3.0.3",
    "agenda": "^5.0.0",
    "nodemailer": "^6.9.7",
    "@sendgrid/mail": "^8.1.0",
    "twilio": "^4.20.0",
    "telegraf": "^4.15.3",
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/bcrypt": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@jest/globals": "^29.7.0",
    "supertest": "^6.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-bundle-analyzer": "^4.10.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "@changesets/cli": "^2.27.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
